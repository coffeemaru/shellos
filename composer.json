{
    "name": "coffeemaru/shellos",
    "description": "A PHP system calls library that expose a simple interface",
    "type": "library",
    "license": "BSD-3-Clause",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Coffeemaru",
            "email": "coffeeunderflow@gmail.com"
        }
    ],
    "autoload": {
        "files": [ "src/functions.php" ],
        "psr-4": {
            "Coffeemaru\\Shellos\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.4",
        "phpseclib/phpseclib": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "scripts": {
        "test": "phpunit --coverage-html coverage"
    }
}
